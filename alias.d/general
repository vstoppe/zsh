### aliases which are used privately and on work:
alias b="cd .."
alias grep="grep --color=always"
alias l="ls -lh"
alias ll="ls -lh"
alias ls="ls -G"
alias nv="nvim"
#alias vim="nvim"
alias vim="/usr/bin/vim -u ~/.vim/vimrc_native"
alias ws="cd ~/workspace"
alias ws3="cd ~/workspace/k3s"
alias wsa="cd ~/workspace/ansible"
alias wsd="cd ~/workspace/docker"
alias wsk="cd ~/workspace/k3os"
alias wsp="cd ~/workspace/puppet"
alias wsp="cd ~/workspace/python"
alias wsr="cd ~/workspace/ruby"
alias wss="cd ~/workspace/scripte"
